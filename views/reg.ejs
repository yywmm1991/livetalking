<form class="form-horizontal" method="post">
	<div class="form-group">
		<label for="inputUserName" class="col-sm-4 control-label">
			User Name
		</label>
		<div class="col-sm-4">
			<input type="text" class="form-control" name="name" id="inputUserName" placeholder="Shawn">
		</div>
	</div>
	<div class="form-group">
		<label for="inputPassword" class="col-sm-4 control-label">
			Password
		</label>
		<div class="col-sm-4">
			<input type="password" class="form-control" name="password" id="inputPassword" placeholder="Password">
		</div>
	</div>
	<div class="form-group">
		<label for="confirmPassword" class="col-sm-4 control-label">
			Confirm
		</label>
		<div class="col-sm-4">
			<input type="password" class="form-control" name="confirm" id="confirmPassword" placeholder="Password">
		</div>
	</div>
	<div class="form-group">
		<div class="col-sm-offset-4 col-sm-4">
			<button type="submit" class="btn btn-info">
				Submit
			</button>
		</div>
	</div>
</form>

<% include footer %>